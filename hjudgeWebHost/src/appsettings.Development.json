{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.EntityFrameworkCore": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDB)\\MSSQLLocalDB;Initial Catalog=hjudge;Connection Timeout=30;"
  },
  "Redis": {
    "Configuration": "127.0.0.1",
    "InstanceName": "hjudge"
  },
  "EmailConfig": {
    "ApiId": "",
    "ApiSec": "",
    "Domain": "hjudge.com",
    "Smtp": {
      "Host": "",
      "Port": 587,
      "EnableSsl": true
    }
  },
  "HostName": "hjudge.com",
  "MessageQueue": {
    "HostName": "localhost",
    "VirtualHost": "/",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "Producers": [
      {
        "Queue": "JudgeQueue",
        "Durable": true,
        "Exclusive": false,
        "AutoDelete": false,
        "Exchange": "JudgeQueue_Exchange",
        "RoutingKey": "H::Judge_JudgeQueue"
      }
    ],
    "Consumers": [
      {
        "Queue": "JudgeReport",
        "Durable": true,
        "Exclusive": false,
        "AutoAck": false,
        "Exchange": "JudgeReport_Exchange",
        "RoutingKey": "H::Judge_JudgeReport"
      }
    ]
  }
}
