<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - H::Judge</title>
    <environment include="Development">
        <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon" asp-append-version="true" />
        <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    </environment>
    <environment exclude="Development">
        <link rel="shortcut icon" href="https://cdn.hjudge.com/hjudge/favicon.ico" type="image/x-icon" asp-append-version="true" asp-fallback-href="~/favicon.ico" />
        <link rel="stylesheet" href="https://cdn.hjudge.com/hjudge/dist/vendor.css" asp-append-version="true" asp-fallback-href="~/dist/vendor.css" />
    </environment>
</head>
<body>
    @RenderBody()

    <environment include="Development">
        <script type="text/javascript" src="~/dist/vendor.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script type="text/javascript" src="https://cdn.hjudge.com/hjudge/dist/vendor.js" asp-append-version="true" asp-fallback-src="~/dist/vendor.js"></script>
    </environment>
    @RenderSection("scripts", required: false)
</body>
</html>
