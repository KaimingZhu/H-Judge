@using hjudge.WebHost.Models.Account
@model UserInfoModel
@{
    ViewData["Title"] = "主页";
}

<environment exclude="Development">
    @Html.ReactRouter("AppComponent", new { UserInfo = Model, Location = $"{Context.Request.Scheme}://{Context.Request.Host}{Context.Request.Path}{Context.Request.QueryString}" });

    @section Scripts {
        @Html.ReactInitJavaScript()
    }
</environment>